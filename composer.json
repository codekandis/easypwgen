{
	"name": "codekandis/easypwgen-api",
	"description": "This is an API to generate easy to read passwords.",
	"keywords": [
		"php",
		"easy",
		"password",
		"generator",
		"consonants",
		"vocals"
	],
	"minimum-stability": "stable",
	"prefer-stable": true,
	"license": "MIT",
	"authors": [
		{
			"name": "Christian Ramelow",
			"email": "info@codekandis.net",
			"homepage": "https://codekandis.net",
			"role": "Developer"
		}
	],
	"support": {
		"email": "info@codekandis.net",
		"issues": "https://github.com/codekandis/easypwgen-api/issues",
		"docs": "https://github.com/codekandis/easypwgen-api/blob/master/README.md",
		"source": "https://github.com/codekandis/easypwgen-api/tree/master"
	},
	"require": {
		"php": "^7.4",
		"codekandis/configurations": "^0",
		"codekandis/tiphy": "^0",
		"codekandis/sentry-client": "^1",
		"codekandis/tiphy-sentry-client-integration": "^0"
	},
	"require-dev": {
		"roave/security-advisories": "dev-master",
		"codekandis/phpunit": "^3"
	},
	"repositories": {
	},
	"autoload": {
		"psr-4": {
			"CodeKandis\\EasyPwGenApi\\": [
				"config/",
				"public/",
				"src/"
			]
		}
	},
	"autoload-dev": {
		"psr-4": {
			"CodeKandis\\EasyPwGenApi\\Tests\\": [
				"tests/"
			]
		}
	}
}
